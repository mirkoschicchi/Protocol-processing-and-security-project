<?import javafx.scene.control.Label?>
<TableView
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:id="table"
        fx:controller="fi.utu.protproc.group3.userinterface.RoutingTableController"
        minHeight="200" prefHeight="188.0" prefWidth="800.0"
        editable="false"
>
    <columns>
        <TableColumn fx:id="prefixColumn" prefWidth="200.0" text="Destination" />
        <TableColumn fx:id="nextHopColumn" prefWidth="200.0" text="Next hop" />
        <TableColumn fx:id="metricColumn" prefWidth="40.0" text="Metric" />
        <TableColumn fx:id="ethernetInterfaceColumn" prefWidth="60.0" text="Intf." />
        <TableColumn fx:id="asPathColumn" prefWidth="300.0" text="AS Path" />
    </columns>
    <items>
    </items>
</TableView>
